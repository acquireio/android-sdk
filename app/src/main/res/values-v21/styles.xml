<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:windowDisablePreview">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <style name="sHeader1AQR">
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">#223263</item>
        <item name="android:fontFamily">@font/poppins_bold</item>
        <item name="android:letterSpacing">0.03</item>
    </style>

    <style name="SubTitleStyle">
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">#9098B1</item>
        <item name="android:letterSpacing">0.04</item>
        <item name="android:fontFamily">@font/poppins_medium</item>
    </style>

    <style name="SubTextStyle1">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#9098B1</item>
        <item name="android:fontFamily">@font/poppins_bold</item>
        <item name="android:letterSpacing">0.04</item>
    </style>

    <style name="sHeader4AQR">
        <item name="android:textSize">16sp</item>
        <item name="android:letterSpacing">0.03</item>
        <item name="android:textColor">#29B68D</item>
    </style>
    <style name="ButtonTextStyle">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:fontFamily">@font/poppins_bold</item>
        <item name="android:letterSpacing">0.04</item>
    </style>

    <style name="EditTextStyle">
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">#9098B1</item>
        <item name="android:fontFamily">@font/poppins_medium</item>
        <item name="android:letterSpacing">0.04</item>
    </style>

    <style name="title">
        <item name="android:textSize">24sp</item>
        <item name="android:letterSpacing">0.02</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:fontFamily">@font/poppins_bold</item>
    </style>

    <style name="a_nice_disc">
        <item name="android:textSize">12sp</item>
        <item name="android:letterSpacing">0.04</item>
        <item name="android:textColor">#EBF0FF</item>
    </style>

</resources>